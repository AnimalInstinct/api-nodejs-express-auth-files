{
  "author": "Alexander Makarov",
  "license": "MIT",
  "name": "api-express-auth-files",
  "version": "1.0.0",
  "description": "API example with JWT auth and files handling",
  "main": "index.ts",
  "scripts": {
    "pretest": "cross-env NODE_ENV=test npm run migrate:reset",
    "test": "cross-env NODE_ENV=test jest --watchAll --no-cache --runInBand --detectOpenHandles --testTimeout=10000",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll --no-cache --runInBand --detectOpenHandles --testTimeout=10000",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "start": "node ./build/server.js",
    "start:dev": "nodemon --exec ts-node --files src/server.ts",
    "build:start": "node ./build/server.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint --fix src/**/*.ts",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:reset": "npx sequelize-cli db:migrate:undo:all && npm run migrate"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./src/test/setup.ts"
    ]
  },
  "dependencies": {
    "@types/cookie-session": "^2.0.41",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.7",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/multer": "^1.4.4",
    "@types/sequelize": "^4.28.9",
    "body-parser": "^1.19.0",
    "cookie-session": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.6.1",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.2",
    "mysql2": "^2.1.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^6.2.0",
    "ts-node-dev": "^1.0.0-pre.56",
    "typescript": "^3.9.7"
  },
  "devDependencies": {
    "@types/jest": "^26.0.10",
    "@types/supertest": "^2.0.10",
    "cross-env": "^7.0.2",
    "jest": "^26.4.2",
    "supertest": "^4.0.2",
    "ts-jest": "^26.2.0"
  }
}
